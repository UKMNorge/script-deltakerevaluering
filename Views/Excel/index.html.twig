		{% include "Excel:row.html.twig"|UKMpath with {col: 'G', group: 'M', header: 'Hva syns du om kveldene i landsbyen, ut fra helhetsvurdering' } %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'M', group: 'L', adjust: [6,6], colAdjust: 80, header: 'Hva syns du om kveldene i landsbyen, ut fra mengde fritid', label: 'Landsby-vurdering'} %}


		{% include "Excel:row.html.twig"|UKMpath with {col: 'K'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'U'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'U', group: 'E', header: 'Hva synes du om korslaget, ut fra type deltaker', label: 'Korslag-vurdering'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'V'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'V', group: 'E', header: 'Hva synes du om åpen scene, ut fra type deltaker'} %}

		{% include "Excel:row.html.twig"|UKMpath with {col: 'T'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'T', group: 'H', header: 'Hva synes du om rumpeldunk, ut fra sliten-skala etter festivalen', label: 'Rumpeldunk-karakter'} %}

		{% include "Excel:row-header.html.twig"|UKMpath with {header: 'Var det lett å finne frem'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'N'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'O'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'P'} %}


		{% include "Excel:row-header.html.twig"|UKMpath with {header: 'Kunstnere'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AG', kunSvar: true} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'J'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'J', group: 'E', header: 'Hva synes du om utstillingen, ut fra type deltaker', label: 'Utstilling-score'} %}

		{% include "Excel:row-header.html.twig"|UKMpath with {header: 'Artister'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AH', kunSvar: true} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AJ', kunSvar: true} %}

		{% include "Excel:row-header.html.twig"|UKMpath with {header: 'Unge arrangører'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AK', kunSvar: true} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AL', kunSvar: true} %}

		{% include "Excel:row-header.html.twig"|UKMpath with {header: 'UKM Media'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AQ', kunSvar: true } %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AR', kunSvar: true} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AS', kunSvar: true} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'L', group: 'AS', kunSvar: true} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AR', group: 'AS', kunSvar: true, header: 'Skille mellom ledere og deltakere, ut fra arbeidsbelastning', label: 'Leder-vurdering'} %}


		{% include "Excel:row-header.html.twig"|UKMpath with {header: 'Spørsmål med ukjent kontekst (hvem gikk dette til?'} %}
		{% include "Excel:row.html.twig"|UKMpath with {col: 'AO'} %}
	</div>
{% endblock %}